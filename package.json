{
  "name": "bala-blu",
  "displayName": "Bala Blu",
  "description": "A Visual Studio Code extension that adds dummy bala blu text in your editor",
  "version": "0.0.1",
  "publisher": "Gideon Idoko",
  "homepage": "https://github.com/IamGideonIdoko/vscode-bala-blu",
  "repository": {
    "type": "git",
    "url": "https://github.com/IamGideonIdoko/vscode-bala-blu"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bala-blu.line",
        "title": "Bala Blu: Add a line"
      },
      {
        "command": "bala-blu.paragraph",
        "title": "Bala Blu: Add a paragraph"
      },
      {
        "command": "bala-blu.manyParagraphs",
        "title": "Bala Blu: Add multiple paragraphs"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts --max-warnings=0",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "prettier": "^2.8.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "lorem-ipsum": "^2.0.8"
  },
  "icon": "https://raw.githubusercontent.com/IamGideonIdoko/vscode-bala-blu/master/images/bala_blue_icon.png"
}
